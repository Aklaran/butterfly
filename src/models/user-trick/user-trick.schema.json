{
	"$jsonSchema": {
		"bsonType": "object",
		"required": [
			"trickName",
			"userID",
			"entryTransitions",
			"landingStances"
		],
		"properties": {
			"id": {
				"bsonType": "objectId"
			},
			"trickName": {
				"bsonType": "string"
			},
			"userID": {
				"bsonType": "string"
			},
			"landingStances": {
				"bsonType": "array",
				"items": {
					"enum": [
						"unified",
						"complete",
						"hyper",
						"semi",
						"mega",
						"turbo"
					]
				}
			},
			"entryTransitions": {
				"bsonType": "object",
				"required": [
					"unified",
					"complete",
					"hyper",
					"semi",
					"mega",
					"turbo"
				],
				"patternProperties": {
					"complete|hyper|semi|mega|turbo|unified": {
						"bsonType": "array",
						"items": {
							"enum": [
								"punch",
								"pop",
								"reverse-pop",
								"bound",
								"backswing",
								"wrap",
								"missleg",
								"rapid",
								"carrythrough",
								"frontswing",
								"masterswing",
								"lotusswing",
								"redirect",
								"vanish",
								"reversal",
								"skip"
							]
						}
					}
				}
			}
		}
	}
}
